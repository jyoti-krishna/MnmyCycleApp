<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.MnmyCycle" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#C0710492</item>
        <item name="colorPrimaryVariant">#24024D</item>
        <item name="colorOnPrimary">@color/black</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">@color/deep_purple</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.MnmyCycle.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    <style name="dialogAnim">
        <item name="android:windowEnterAnimation">@anim/slidein</item>
        <item name="android:windowExitAnimation">@anim/slideout</item>
    </style>

<!--    <style name="Theme.MnmyCycle.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />-->

<!--    <style name="Theme.MnmyCycle.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />-->
</resources>